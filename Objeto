public class Main {

    public static void main (String [] args){

        Persona persone = new Persona(25,"DIEGO","3516465213");


        System.out.println(persone.getNombre());
        System.out.println(persone.getEdad());
        System.out.println(persone.getTelefono());


    }

}

 class Persona{
    //atributos privados y se accede por medio de setter y getters
    private int edad;
    private String nombre;
    private String telefono;

    //Generar primero el constructor para inicializar el objeto



     public Persona(int edad, String nombre,String telefono){

         this.edad = edad;
         this.nombre = nombre;
         this.telefono = telefono;


     }



     public void setEdad(int edad) {
         this.edad = edad;
     }

     public void setNombre(String nombre){
         this.nombre = nombre;
     }

     public void setTelefono(String telefono){
         this.telefono = telefono;
     }

     public int getEdad() {
         return edad;
     }

     public String getNombre() {
         return nombre;
     }

     public String getTelefono(){
         return telefono;
     }
 }
